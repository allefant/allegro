import os
env = Environment(ENV = os.environ)

path = "#docs/naturaldocs"

public_docs = env.Command(Dir(path + "/public"), None,
   "make public -C docs/naturaldocs")
private_docs = env.Command(Dir(path + "/private"), None,
   "make private -C docs/naturaldocs")

docs = [public_docs, private_docs]
env.Alias("naturaldocs", docs)
Return('docs')
